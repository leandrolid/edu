{
  "name": "edu",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --log-prefix=none --log-order=stream",
    "migrate:dev": "turbo run migrate:dev",
    "seed:dev": "turbo run seed:dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "turbo": "^2.4.4",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*",
    "config/*"
  ]
}
